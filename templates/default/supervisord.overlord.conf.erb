[program:overlord]
command = /usr/bin/java -Xmx256m -Duser.timezone=UTC -Dfile.encoding=UTF-8 -classpath /var/druid-<%=@version%>/lib/*:/var/druid-<%=@version%>/config/overlord io.druid.cli.Main server overlord
process_name=%(program_name)s
autostart=true
autorestart=true
stopsignal=KILL
stopasgroup = true
killasgroup = true


stdout_logfile=/tmp/overlord.log
stdout_logfile_maxbytes=1MB
stdout_logfile_backups=1

stderr_logfile = /tmp/overlord.err
stderr_logfile_maxbytes=1MB
stderr_logfile_backups=1


[group:overlord_server]
programs=overlord




