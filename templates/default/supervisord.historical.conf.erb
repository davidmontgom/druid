[program:historical]
command = /usr/bin/java -Xmx256m -Duser.timezone=UTC -Dfile.encoding=UTF-8 -classpath /var/druid-<%=@version%>/lib/*:/var/druid-<%=@version%>/config/historical io.druid.cli.Main server historical

process_name=%(program_name)s
autostart=true
autorestart=true
stopsignal=KILL
stopasgroup = true
killasgroup = true


stdout_logfile=/tmp/historical.log
stdout_logfile_maxbytes=1MB
stdout_logfile_backups=1

stderr_logfile = /tmp/historical.err
stderr_logfile_maxbytes=1MB
stderr_logfile_backups=1


[group:historical_server]
programs=historical




