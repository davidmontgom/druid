[program:middleManager]

command = /usr/bin/java -Xmx256m -Duser.timezone=UTC -Dfile.encoding=UTF-8 -classpath /var/druid-<%=@version%>/lib/*:/var/druid-<%=@version%>/config/middleManager io.druid.cli.Main server middleManager



process_name=%(program_name)s
autostart=true
autorestart=true
stopsignal=KILL
stopasgroup = true
killasgroup = true


stdout_logfile=/tmp/middleManager.log
stdout_logfile_maxbytes=1MB
stdout_logfile_backups=1

stderr_logfile = /tmp/middleManager.err
stderr_logfile_maxbytes=1MB
stderr_logfile_backups=1


[group:middleManager_server]
programs=middleManager




