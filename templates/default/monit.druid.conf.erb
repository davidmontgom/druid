#monit procmatch ".*/var/druid-<%=@version%>/config/<%=@role%> io.druid.cli.Main server <%=@role%>"
check process <%=@role%>
	with matching ".*/var/druid-<%=@version%>/config/<%=@role%> io.druid.cli.Main server <%=@role%>"
	start program = "/usr/local/bin/supervisorctl start <%=@role%>_server:"
	stop program = "/usr/local/bin/supervisorctl stop <%=@role%>_server:"

